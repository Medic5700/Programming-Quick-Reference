10 FOR I = 0 TO 10
20   X = I
30   GOSUB 1000
40   PRINT X; "!"; "    ="; Y
50 NEXT
60 END

1000 REM FIB2, (X) INPUT, (Y) OUTPUT
1010 IF X = 0 THEN Y = 0     : RETURN
1020 IF X = 1 THEN Y = 1     : RETURN
1030 A = 0
1040 B = 1
1050 Y = 1
1060 FOR J = 2 TO X
1070   Y = A + B
1080   A = B
1090   B = Y
1100 NEXT
1110 RETURN
